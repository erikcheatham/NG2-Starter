    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "0.2.2",
    "ng2-validation": "^3.1.3",
    "protractor": "4.0.14",
    "ts-node": "1.2.1",
    "tslint": "4.4.2",
    "typescript": "2.1.5",
	"karma-trx-reporter": "^0.2.9"
