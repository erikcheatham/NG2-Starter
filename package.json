{
  "name": "ng2-starter",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --browsers=Chrome",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "vstsTest": "ng test --watch=false --browsers=PhantomJS",
    "deployDev": "ng build  --env=dev --aot=true --sourcemap=false",
    "deployTest": "ng build --target=production  --env=test --aot=true --sourcemap=false",
    "deployProd": "ng build --target=production --env=prod --aot=true --sourcemap=false"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.2",
